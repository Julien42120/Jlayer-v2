<form method="post" action="{{ path('user_delete', {'id': user.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer votre profil ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">

	<button type="submit" class="btn buttonDelete">Supprimer profil</button>

</form>
