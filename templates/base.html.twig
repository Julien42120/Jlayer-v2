<!doctype html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>
			{% block title %}Jlayers
			{% endblock %}
		</title>
		<link rel="icon" type="image/x-icon" href="{{ asset ('assets/images/logoOnglet.png')}}">
		{% block stylesheets %}
			<link href="{{ asset ("assets/css/styles.css")}}" rel="stylesheet">

			<script src="https://unpkg.com/rive-js"></script>

			<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
			<link rel="preconnect" href="https://fonts.googleapis.com">
			<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
			<link href="https://fonts.googleapis.com/css2?family=Dosis:wght@500&display=swap" rel="stylesheet">
			<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
			<link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css">
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
			<link rel="stylesheet" href="path/to/font-awesome/css/font-awesome.min.css">
			<link type="text/css" rel="stylesheet" href="{{ asset ("assets/css/styles.css")}}"/>
			<script src='https://kit.fontawesome.com/a076d05399.js' crossorigin='anonymous'></script>
			<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
		{% endblock %}


		<!--NAVBAR-->
		<header>
			<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
				<div class="container-fluid">
					<a class="navbar-brand" href="{{path ('home')}}"><img class="LogoJlayers" src="{{ asset ('assets/images/logoOnglet.png')}}" alt="Logo Jlayers"/></a>
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarSupportedContent">
						<ul class="navbar-nav me-auto mb-2 mb-lg-0">
							<li class="nav-item">
								<a class="nav-link active" aria-current="page" href="{{ path ('home')}}">Accueil</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="{{ path ('bibliotheque')}}">Bibliothéque STL</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="{{ path ('mail')}}">Contact</a>
							</li>
							<li class="nav-item dropdown">
								<a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
									Infos
								</a>
								<ul class="dropdown-menu" aria-labelledby="navbarDropdown">
									<li>
										<a class="dropdown-item" href="{{ path ('impression_fil')}}">Impressions filaments</a>
									</li>
									<li>
										<a class="dropdown-item" href="{{ path ('impression_resine')}}">Impressions résines</a>
									</li>
								</ul>
							</li>
						</ul>
						<ul class="navbar-nav ms-auto mb-2 mb-lg-0 profile-menu">
							<li class="nav-item dropdown">
								<a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
									{% if app.user %}
										<img class="Avatar" src="{{ asset ('assets/upload/photosUsers/' ~ app.user.avatar)}}"/>
									{% else %}
										<i class="fas fa-user"></i>
									{% endif %}
								</a>
								<ul class="dropdown-menu" aria-labelledby="navbarDropdown">
									{% if app.user %}
										<li>
											<a class="dropdown-item" href="{{ path ('user_index') }}">
												<i class="fas fa-sliders-h fa-fw"></i>
												Profil</a>
										</li>
									{% else %}
										<li>
											<a class="dropdown-item" href="{{ path ('app_register') }}">
												<i class="fas fa-cog fa-fw"></i>
												Inscription / Connexion</a>
										</li>
									{% endif %}
									{% if app.user %}
										<li><hr class="dropdown-divider"></li>
										<li>
											<a class="dropdown-item" href="{{ path ('app_logout') }}" style="color:red;">
												<i class="fas fa-sign-out-alt fa-fw" style="color:red;"></i>
												Déconnexion</a>
										</li>
									{% endif %}
								</ul>
							</li>
						</li>
					</ul>
				</div>
			</div>
		</nav>
		<!--FIN NAVBAR-->
	</head>
	<body>
		{% block body %}{% endblock %}
		<script src="{{ asset ("assets/js/Login.js")}}"></script>
		<script src="https://preview.babylonjs.com/viewer/babylon.viewer.js"></script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
		<script src="{{ asset ("assets/js/download.js")}}"></script>
		<script src="{{ asset ("assets/js/like.js")}}"></script>
		<script src="{{ asset ("assets/js/TriCategory.js")}}"></script>
		<script src="{{ asset ("assets/js/navbar.js")}}"></script>

		<script src="https://unpkg.com/rive-js"></script>

		{% block javascripts %}{% endblock %}
		{# <!----------------FOOTER--------------------> #}
		<div class="footer">
			<h1>Develloppé par Julien Martin</h1>
			<img class="logo" src="{{ asset ('assets/images/Jlayers (1).jpg')}}">
		</div>
		{# <!----------------FIN FOOTER--------------------> #}
	</body>
</html></head></html></head></html>
